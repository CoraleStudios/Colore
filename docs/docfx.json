{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "Corale.Colore/bin/Release/netstandard1.3/Corale.Colore.dll"
          ],
          "exclude": [ "**/obj/**" ]
        }
      ],
      "dest": "api",
      "properties": { "TargetFramework": "netstandard1.3" }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
      {
        "src": "..",
        "files": [
          "*.md",
          "LICENSE",
          "AUTHORS",
          "CODEOWNERS"
        ],
        "dest": "./"
      },
      {
        "src": "../.github",
        "files": [ "CODE_OF_CONDUCT.md" ],
        "dest": "./"
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [ "apidoc/**.md" ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appLogoPath": "images/colore.png",
      "_enableSearch": true
    },
    "fileMetadataFiles": [],
    "template": [ "default", "theme" ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "sitemap": {
      "baseUrl": "https://coralestudios.github.io/Colore/docs/"
    }
  }
}
